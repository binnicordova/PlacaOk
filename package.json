{
  "name": "placaok",
  "main": "expo-router/entry",
  "version": "0.0.1",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "pnpx @biomejs/biome format --write ./src",
    "build:dev": "eas build --platform all --profile development",
    "build:preview": "eas build --platform all --profile preview",
    "build:prod": "eas build --platform all --profile production",
    "build:prod:android": "eas build --platform android --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "update:dev": "eas update --branch development --message \"Development update - $(date '+%Y-%m-%d %H:%M')\"",
    "update:preview": "eas update --branch preview --message \"Preview update - $(date '+%Y-%m-%d %H:%M')\"",
    "update:prod": "eas update --branch production --message \"Production update - $(date '+%Y-%m-%d %H:%M')\"",
    "update:prod:android": "eas update --branch production --platform android --message \"Production Android update - $(date '+%Y-%m-%d %H:%M')\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/analytics": "^23.4.0",
    "@react-native-seoul/masonry-list": "^1.4.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~54.0.12",
    "expo-application": "~7.0.7",
    "expo-background-task": "~1.0.8",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-task-manager": "^14.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "firebase": "^12.4.0",
    "jotai": "^2.12.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.1",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "2.2.5",
    "@types/react": "~19.1.17",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "sharp": "^0.34.4",
    "typescript": "~5.9.3"
  },
  "private": true
}